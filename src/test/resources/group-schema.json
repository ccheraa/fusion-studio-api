  "title": "User",
  "type": "object",
  "required": [
    "groupName",
    "managers"
  ],
  "properties": {
    "groupName": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "managers": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "metadata": {
      "type": "array",
      "items": {
        "$ref": "attribute-schema.json"
      }
    }
  }
}